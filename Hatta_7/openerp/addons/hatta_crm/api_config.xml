<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_base_config_api_key" model="ir.ui.view">
            <field name="name">base.config.api.key</field>
            <field name="model">base.config.settings</field>
            <field name="inherit_id" ref="base_setup.view_general_configuration"/>
            <field name="arch" type="xml">
                <xpath expr="//group[last()]" position="after">
                    <group>
                        <label for="id" string="PDF To DOC Converter API Key"/>
                        <div>
                            <div>
                                <field name="pdf_doc_api_key" class="oe_inline"/>
                            </div>
                        </div>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
